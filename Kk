local function ShowMenuPanel()
    local existed = gui:FindFirstChild("KS_MenuPanel")
    if existed then
        existed.Visible = true
        return
    end

    local p = Instance.new("Frame")
    p.Name = "KS_MenuPanel"
    p.Size = UDim2.new(0, 420, 0, 300)
    p.Position = UDim2.new(0.5, -210, 0.5, -150)
    p.BackgroundColor3 = Color3.fromRGB(20, 22, 26)
    p.BorderSizePixel = 0
    p.Parent = gui
    Instance.new("UICorner", p).CornerRadius = UDim.new(0, 14)
    local ps = Instance.new("UIStroke", p)
    ps.Color = Color3.fromRGB(85, 85, 95)
    ps.Thickness = 1

    local bar = Instance.new("Frame", p)
    bar.Size = UDim2.new(1, 0, 0, 42)
    bar.BackgroundColor3 = Color3.fromRGB(28, 30, 36)
    bar.BorderSizePixel = 0
    bar.Active = true
    Instance.new("UICorner", bar).CornerRadius = UDim.new(0, 14)

    local ttl = Instance.new("TextLabel", bar)
    ttl.BackgroundTransparency = 1
    ttl.Position = UDim2.new(0, 14, 0, 0)
    ttl.Size = UDim2.new(1, -60, 1, 0)
    ttl.Font = Enum.Font.GothamBold
    ttl.TextSize = 18
    ttl.TextColor3 = Color3.fromRGB(235, 235, 245)
    ttl.TextXAlignment = Enum.TextXAlignment.Left
    ttl.Text = "Menu"

    dragify(bar, p)

    local close = Instance.new("TextButton", bar)
    close.AnchorPoint = Vector2.new(1, 0.5)
    close.Position = UDim2.new(1, -8, 0.5, 0)
    close.Size = UDim2.new(0, 28, 0, 28)
    close.BackgroundColor3 = Color3.fromRGB(45, 45, 50)
    close.Text = "‚úï"
    close.TextColor3 = Color3.fromRGB(255, 255, 255)
    close.Font = Enum.Font.GothamBold
    close.TextSize = 16
    Instance.new("UICorner", close).CornerRadius = UDim.new(1, 0)
    close.MouseButton1Click:Connect(function()
        p.Visible = false
    end)

    local loader = Instance.new("Frame", p)
    loader.Size = UDim2.new(1, -24, 0, 76)
    loader.Position = UDim2.new(0, 12, 0, 62)
    loader.BackgroundTransparency = 1

    local lText = Instance.new("TextLabel", loader)
    lText.BackgroundTransparency = 1
    lText.Size = UDim2.new(1, 0, 0, 24)
    lText.Position = UDim2.new(0, 0, 0, 0)
    lText.Font = Enum.Font.GothamBold
    lText.TextSize = 18
    lText.TextColor3 = Color3.fromRGB(230, 230, 235)
    lText.Text = "Loading Menu... 1%"

    local lBg = Instance.new("Frame", loader)
    lBg.AnchorPoint = Vector2.new(0.5, 0)
    lBg.Position = UDim2.new(0.5, 0, 0, 34)
    lBg.Size = UDim2.new(0.9, 0, 0, 24)
    lBg.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    lBg.BorderSizePixel = 0
    Instance.new("UICorner", lBg).CornerRadius = UDim.new(0, 12)

    local lFill = Instance.new("Frame", lBg)
    lFill.Size = UDim2.new(0, 0, 1, 0)
    lFill.BackgroundColor3 = Color3.fromRGB(114, 106, 240)
    lFill.BorderSizePixel = 0
    Instance.new("UICorner", lFill).CornerRadius = UDim.new(0, 12)

    local content = Instance.new("Frame", p)
    content.BackgroundTransparency = 1
    content.Size = UDim2.new(1, -24, 1, -110)
    content.Position = UDim2.new(0, 12, 0, 100)
    content.Visible = false

    local list = Instance.new("UIListLayout", content)
    list.Padding = UDim.new(0, 10)
    list.HorizontalAlignment = Enum.HorizontalAlignment.Center

    local function menuButton(txt, cb)
        local b = Instance.new("TextButton", content)
        b.Size = UDim2.new(1, 0, 0, 40)
        b.BackgroundColor3 = Color3.fromRGB(35, 36, 40)
        b.Text = txt
        b.TextColor3 = Color3.fromRGB(255, 255, 255)
        b.Font = Enum.Font.GothamSemibold
        b.TextSize = 16
        Instance.new("UICorner", b).CornerRadius = UDim.new(0, 10)
        local st = Instance.new("UIStroke", b)
        st.Color = Color3.fromRGB(84, 130, 255)
        st.Thickness = 1
        if cb then
            b.MouseButton1Click:Connect(cb)
        end
        return b
    end

    task.spawn(function()
        for i = 1, 100 do
            lText.Text = "Loading Menu... " .. i .. "%"
            lFill.Size = UDim2.new(i / 100, 0, 1, 0)
            task.wait(0.02) -- 2s
        end
        loader.Visible = false
        content.Visible = true

        menuButton("üß† Add Brainrot (P)", function()
            VIM:SendKeyEvent(true, Enum.KeyCode.P, false, game)
            VIM:SendKeyEvent(false, Enum.KeyCode.P, false, game)
        end)

        menuButton("üß† Duplicate v2 (10s + Script)", function()
            ShowProgress10s(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/tunadan212/Kkkk/refs/heads/main/K"))()
            end)
        end)

        menuButton("üéâ Execute Event (Q)", function()
            VIM:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
            VIM:SendKeyEvent(false, Enum.KeyCode.Q, false, game)
        end)

        menuButton("ü§§ Spawn Brainrot (R)", function()
            VIM:SendKeyEvent(true, Enum.KeyCode.R, false, game)
            VIM:SendKeyEvent(false, Enum.KeyCode.R, false, game)
        end)

        menuButton("üçÄ Open Lucky Block Panel", function()
            ShowLuckyPanel()
        end)

        menuButton("üõí Auto Buy Dealer", function()
            loadstring(game:HttpGet("https://pandadevelopment.net/virtual/file/7da892ca6a02154d"))()
        end)
    end)
end

