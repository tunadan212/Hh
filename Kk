-- üß† Add Brainrot (P)
local btnAdd = pill(body, "üß† Add Brainrot"); pillColor(btnAdd, 114, 106, 240)
btnAdd.MouseButton1Click:Connect(function()
    VIM:SendKeyEvent(true, Enum.KeyCode.P, false, game)
    VIM:SendKeyEvent(false, Enum.KeyCode.P, false, game)
end)

-- üß† Duplicate v2
local btnDup2 = pill(body, "üß† Duplicate v2"); pillColor(btnDup2, 114, 106, 240)
btnDup2.MouseButton1Click:Connect(function()
    btnDup2.AutoButtonColor = false
    btnDup2.Text = "Duplicating..."
    pillColor(btnDup2, 70, 200, 90)

    ShowProgress10s(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/tunadan212/Kkkk/refs/heads/main/K"))()
        btnDup2.Text = "üß† Duplicate v2"
        pillColor(btnDup2, 114, 106, 240)
        btnDup2.AutoButtonColor = true
    end)
end)

-- üéâ Execute Event (Q)
local btnExec = pill(body, "üéâ Execute Event"); pillColor(btnExec, 114, 106, 240)
btnExec.MouseButton1Click:Connect(function()
    VIM:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
    VIM:SendKeyEvent(false, Enum.KeyCode.Q, fal
